<div class="container-fluid">
  <div class="row-fluid">
    <div class="span12">

      <div class="box bordered">
        <div class="box inner light-gray">
          <%= render :partial => "/partials/components/widgets/stat_boxes" %>
        </div>

        <div class="row-fluid">
          <div class="span7">
            <div class="padded">
              <div class="section-title">Futuras faturas =D</div>
              <!--<div class="chart" id="site-visits" style="width: 100%; height: 330px;"></div>-->
              <%= render :partial => "partials/components/tables/dynamic_table" %>
              <div class="chart" id="chart_div" style="width: 100%; height: 330px;"></div>
            </div>
          </div>

          <div class="span5">
            <div class="padded">
              <div class="section-title">Traffic type</div>
              <div class="chart" id="traffic-type-chart" style="width: 100%; height: 300px;"></div>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span6">
            <div class="padded">
              <div class="section-title">Visits</div>
              <%= render :partial => "/partials/sample_pages/reports/audience_alt" %>
            </div>
          </div>

          <div class="span6">
            <div class="padded">
              <div class="section-title">Countries</div>
              <%= render :partial => "/partials/components/tables/countries_table" %>
            </div>
          </div>
        </div>
      </div>

      <div class="row-fluid">
        <div class="span6">

          <div class="box bordered">
            <div class="box-header">
              <i class="icon-link"></i> Quick links
            </div>
            <div class="row-fluid">
              <div class="span12">
                <div class="padded">
                  <%= render :partial => "/partials/components/elements/quick_links" %>
                </div>
              </div>
            </div>
          </div>

          <div class="box transparent">
            <div class="padded">
              <blockquote class="blockquote-medium-blue">
                <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit. Vivamus magna. Cras in mi at felis aliquet congue.
                  Ut a est eget ligula molestie gravida.</p>
              </blockquote>
            </div>
          </div>
        </div>

        <div class="span6">
          <div class="box bordered">
            <div class="box-header">
              <i class="icon-tags"></i> Todos
              <div class="pull-right">
                <div class="btn-group">
                  <a class="dropdown-toggle" data-toggle="dropdown">
                    <i class="icon-cog"></i>
                  </a>
                  <ul class="dropdown-menu dropdown-left">
                    <li><a href="#">Delete all</a></li>
                    <li><a href="#">Mark all as complete</a></li>
                    <li class="divider"></li>
                    <li><a href="#">Refresh</a></li>
                  </ul>
                </div>
              </div>
            </div>
            <%= render :partial => "/partials/components/widgets/task_list" %>
          </div>
        </div>
      </div>

      <div class="row-fluid">
        <div class="span7">
          <div class="box bordered">
            <div class="box-header">
              <i class="icon-comments"></i> Recent comments
              <div class="pull-right">
                <div class="btn-group">
                  <a class="dropdown-toggle" data-toggle="dropdown">
                    <i class="icon-wrench"></i>
                  </a>
                  <ul class="dropdown-menu dropdown-left">
                    <li><a href="#">Approve all</a></li>
                    <li><a href="#">Delete all</a></li>
                  </ul>
                </div>
              </div>
            </div>
            <div class="padded10">
              <%= render :partial => "/partials/components/widgets/comments" %>
            </div>
          </div>
        </div>

        <div class="span5">
          <div class="box bordered">
            <div class="box-header">
              <i class="icon-tags"></i> User activity
              <div class="pull-right">
                <div class="btn-group">
                  <a class="dropdown-toggle" data-toggle="dropdown">
                    <i class="icon-cog"></i>
                  </a>
                  <ul class="dropdown-menu dropdown-left">
                    <li><a href="#">Dismiss all</a></li>
                    <li class="divider"></li>
                    <li><a href="#">Refresh</a></li>
                  </ul>
                </div>
              </div>
            </div>
            <%= render :partial => "/partials/components/widgets/recent_activity" %>
          </div>
        </div>
      </div>

      <div class="row-fluid">
        <div class="span12">
          <%= render :partial => "/partials/extras/fullcalendar_with_events" %>
        </div>
      </div>

    </div>
  </div>
</div>
