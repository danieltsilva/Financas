<div class="container-fluid">
  <div class="row-fluid">
    <div class="span12">

      <div class="box bordered">
        <div class="box-header">
          <i class="icon-align-left"></i> Monthly stats
        </div>

        <div class="row-fluid">
          <div class="span4">
            <div class="padded">
              <div class="section-title"> Audience</div>
              <%= partial "/partials/sample_pages/reports/audience" %>
            </div>
          </div>
          <div class="span8">
            <div class="padded">
              <div class="section-title"> Visitor Stats</div>
              <div class="chart" id="area-chart" style="width: 100%; height: 330px;"></div>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span6">
            <div class="padded">
              <div class="section-title"> Traffic Stats</div>
              <div class="padded">
                <%= partial "/partials/sample_pages/reports/traffic_pie_chart" %>
              </div>
            </div>
          </div>
          <div class="span6">
            <div class="padded">
              <div class="section-title"> Countries</div>
              <%= partial "/partials/components/tables/countries_table" %>
            </div>
          </div>
        </div>

        <div class="row-fluid">
          <div class="span6">
            <div class="padded">
              <div class="section-title"> Browsers</div>
              <div class="padded">
                <%= partial "/partials/sample_pages/reports/browsers_chart" %>
              </div>
            </div>
          </div>
          <div class="span6">
            <div class="padded">
              <div class="section-title"> Browser stats</div>
              <%= partial "/partials/components/tables/browser_stats_table" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
